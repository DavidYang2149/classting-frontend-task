    "@types/redux-mock-store": "^1.0.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.11",
    "babel-jest": "^27.3.1",
    "cypress": "^10.4.0",
