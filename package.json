    "react-confetti": "^6.1.0",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "usehooks-ts": "^2.9.1",
    "victory": "^36.6.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
